import java.util.ArrayList;

public class Test {
	public void UI() {
		String msg = "Hello";
		ArrayList<String> msg2 = new ArrayList<String>(2);
		msg2.add("Hello");
		msg2.add("World");
		UI ui = new UI();
		ui.titleShow(msg);
		ui.unorderedListShow(msg2);
		ui.orderedListShow(msg2);
		ui.errorMsg(msg);
		ui.print();
	}
	public void IO() {
		IO io = new IO();
		io.getInput();
	}
	public static void main(String[] args) {
		IO();
	}
}
